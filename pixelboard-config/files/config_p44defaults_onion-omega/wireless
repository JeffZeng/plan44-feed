config wifi-device 'radio0'
    option type 'mac80211'
    option hwmode '11g'
    option path 'platform/ar933x_wmac'
    option htmode 'HT20'
    option channel '9'
    option txpower '30'
    option disabled '1'

config wifi-iface
    option device 'radio0'
    option network 'wlan'
    option mode 'ap'
    option encryption 'none'
    option ssid 'PixelBoard'
    option disabled '1'
