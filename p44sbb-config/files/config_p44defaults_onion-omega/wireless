config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT20'
	option channel '9'
	option txpower '30'
	option disabled '0'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option network 'wlan'
	option ssid 'P44SBB'
	option encryption 'none'

config wifi-iface
	option device 'radio0'
	option mode 'sta'
	option network 'wwan'
	option ssid '33C3-open-legacy'
	option encryption 'none'
